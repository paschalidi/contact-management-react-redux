  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "enzyme": "^2.8.0",
    "enzyme-to-json": "^1.5.0",
    "jest": "^19.0.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.0",
    "react-test-renderer": "^15.4.0",
    "redux-form": "^6.6.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "babel-preset-stage-1": "^6.1.18",
    "lodash": "^3.10.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4"
  }
